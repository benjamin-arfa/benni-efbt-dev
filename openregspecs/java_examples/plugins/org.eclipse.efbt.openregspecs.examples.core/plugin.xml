<plugin>	
	<extension
          point="org.eclipse.ui.newWizards">
       <category 
            id="org.eclipse.efbt.Examples"
            name="EFBT Example"
            parentCategory="org.eclipse.ui.Examples" />
       <wizard
             category="org.eclipse.ui.Examples/org.eclipse.efbt.Examples"
             class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard"
             icon="icons/EObject.gif"
             id="org.eclipse.efbt.wizard.EFBTExampleWizard"
             name="EFBT Example"
             project="true">
       </wizard>      
    </extension>
       <extension point="org.eclipse.emf.common.ui.examples">
        <example wizardID="org.eclipse.efbt.wizard.EFBTExampleWizard" pageImage="icons/EObject.gif">
            <projectDescriptor name="org.eclipse.efbt.example.frtb" contentURI="examples/org.eclipse.efbt.example.frtb/" description="FRTB LDM Example"/>   
            <projectDescriptor name="org.eclipse.efbt.example.frtb.xcore.fe_input_layer" contentURI="examples/org.eclipse.efbt.example.frtb.xcore.fe_input_layer/" description="FRTB LDM/IL FE Example"/>                         
            <projectDescriptor name="org.eclipse.efbt.example.input_layer_rpmn" contentURI="examples/org.eclipse.efbt.example.input_layer_rpmn/" description="FRTB IL Example"/>
            <projectDescriptor name="org.eclipse.efbt.example.computation_model" contentURI="examples/org.eclipse.efbt.example.computation_model/" description="FRTB Computation Example"/>
        </example>
    </extension>
  
</plugin>
