    <eStructuralFeatures xsi:type="ecore:EAttribute" name="quantity" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="trade_id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="SIZE">
    <eLiterals name="LARGE" value="1" literal="LARGE"/>
    <eLiterals name="MEDIUM" value="2" literal="MEDIUM"/>
    <eLiterals name="SMALL" value="3" literal="SMALL"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Entity_derived">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="size" eType="#//SIZE"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="entity_id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Trade_derived">
