    <eStructuralFeatures xsi:type="ecore:EAttribute" name="MARKET_VALUE" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
  </eClassifiers>
</ecore:EPackage>
