<plugin>	
	<extension
          point="org.eclipse.ui.newWizards">
       <category 
            id="org.eclipse.efbt.cocason.dsl.cocason.ui.Examples"
            name="CoCaSon"
            parentCategory="org.eclipse.ui.Examples" />
       <wizard
             category="org.eclipse.ui.Examples/org.eclipse.efbt.cocason.dsl.cocason.ui.Examples"
             class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard"
             icon="icons/EObject.gif"
             id="org.eclipse.efbt.cocason.dsl.cocason.ui.wizard.CocasonExampleWizard"
             name="CoCaLiMo Example"
             project="true">
       </wizard>      
