            <projectDescriptor name="org.eclipse.efbt.cocalimo.example.standard" contentURI="examples/org.eclipse.efbt.cocalimo.example.standard/" description="CoCaLiMo Standard Example 1"/>
            <projectDescriptor name="org.eclipse.efbt.cocason.example.incremental" contentURI="examples/org.eclipse.efbt.cocason.example.incremental/" description="CoCaSon Incremental Example 1"/>
            <projectDescriptor name="org.eclipse.efbt.cocason.example.advanced" contentURI="examples/org.eclipse.efbt.cocason.example.advanced/" description="CoCaSon Advanced Example 1"/>
            <projectDescriptor name="org.eclipse.efbt.cocason.example.frtb" contentURI="examples/org.eclipse.efbt.cocason.example.frtb/" description="CoCaSon FRTB Example 1"/>
            <projectDescriptor name="org.eclipse.efbt.cocason.example.bird_import" contentURI="examples/org.eclipse.efbt.cocason.example.bird_import/" description="CoCaSon BIRD Import Example 1"/>
            <projectDescriptor name="org.eclipse.efbt.cocason.example.ldm" contentURI="examples/org.eclipse.efbt.cocason.example.ldm/" description="CoCaSon LDM Example 1"/>                              
        </example>
