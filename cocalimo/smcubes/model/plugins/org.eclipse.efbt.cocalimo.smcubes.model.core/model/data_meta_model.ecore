      <details key="baseType" value="http://www.w3.org/2001/XMLSchema#boolean"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="Int" instanceTypeName="NA">
    <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
